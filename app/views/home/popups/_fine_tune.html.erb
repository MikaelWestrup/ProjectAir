<div class="modal fade" id="fineTunePopup" tabindex="-1" role="dialog" aria-labelledby="fineTunePopupLabel">
  <div class="modal-dialog" role="document">
    <div class="cal-outer-box main-span mCustomScrollbar" style="max-width: 420px;">
      <div class="fine-tune" ng-repeat="ft in fine_tunes">
        <div class="select-date">
          <h4 ng-click="aa(true, $index)" style="cursor: pointer;">{{ show_date(ft.date) || 'Select Date' }}</h4>
          <div class="date-section">
            <div finetune-datepicker="" ng-model="ft.date" ng-class="{'hide' : $index != open_id}" ng-change="aa(false, null)"></div>
          </div>
        </div>
        <div class="row">
          <div class="col-md-4">
            <p>Start</p>
            <div class="form-group">
              <select-hour-time ng-model="ft.start_time.hour"></select-hour-time>
              <select-minute-time ng-model="ft.start_time.min"></select-minute-time>
            </div>
          </div>
          <div class="col-md-4">
            <p>Stop</p>
            <div class="form-group">
              <select-hour-time ng-model="ft.end_time.hour"></select-hour-time>
              <select-minute-time ng-model="ft.end_time.min"></select-minute-time>
            </div>
          </div>
          <div class="col-md-4">
            <p>Paragraphs</p>
            <div class="form-group">
              <select name="colors" class="form-control fc-lg" ng-model="ft.paragraph" ng-options="item.id as item.name for item in list2">
              </select>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-md-12">
            <p>Note</p>
            <input type="text" class="form-control" ng-model="ft.note" />
          </div>
        </div><br>
      </div>
      <div class="footer-cal">
        <button type="button" class="btn btn-default" ng-click="addFineTune()">+</button>
      </div>
      <br>
      <div class="footer-cal">
        <button type="button" class="btn btn-cal" data-dismiss="modal">Done</button>
      </div>
    </div>
  </div>
</div>
